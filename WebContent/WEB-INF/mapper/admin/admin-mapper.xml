<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<entry key="adminRegist">
	INSERT
	INTO admin
	(
	   admin_id
	  ,admin_pwd
	  ,admin_name
	  ,admin_email
	  ,admin_date
	  ,admin_role
	)
	VALUES
	(
	   ?
	  ,?
	  ,?
   	  ,?
	  ,now()
	  ,?
	)
	</entry>
	
	<entry key="adminIdSelect">
	    SELECT 
	           COUNT(*)
	      FROM ADMIN
	      WHERE admin_id = ?
	</entry>
	
	<entry key="selectAdminList">
	    SELECT 
	         *
	      FROM ADMIN
	      LIMIT ?,?
	</entry>
	
	<entry key="selectTotalCount">
        SELECT 
               COUNT(*)
          FROM ADMIN
	</entry>
	<entry key="selectOneAdminId">
		SELECT 
		       *
		  FROM ADMIN
		 WHERE ADMIN_ID = ?
		
	</entry>
	<entry key="updateAdmin">
	    UPDATE 
	            ADMIN
	       SET  ADMIN_PWD = ? 
	           ,ADMIN_NAME = ? 
	           ,ADMIN_EMAIL = ? 
	           ,ADMIN_DATE = ? 
	           ,ADMIN_ROLE = ? 
	      WHERE ADMIN_ID = ?
	
	</entry>
	
	<entry key="adminDelete">
		DELETE 
		  FROM ADMIN
		  WHERE ADMIN_ID = ?	
	</entry>
	
	
</properties>